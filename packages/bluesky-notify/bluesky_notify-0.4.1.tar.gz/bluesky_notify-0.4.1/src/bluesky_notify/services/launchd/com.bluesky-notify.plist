<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.bluesky-notify</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/bluesky-notify</string>
        <string>start</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>~/Library/Logs/bluesky-notify.log</string>
    <key>StandardErrorPath</key>
    <string>~/Library/Logs/bluesky-notify.error.log</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>DOCKER_CONTAINER</key>
        <string>false</string>
    </dict>
</dict>
</plist>
