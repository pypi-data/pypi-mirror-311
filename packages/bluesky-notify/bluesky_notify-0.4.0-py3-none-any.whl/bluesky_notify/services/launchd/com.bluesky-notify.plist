<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.bluesky-notify</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/bluesky-notify</string>
        <string>start</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Users/jerdog/Library/Logs/bluesky-notify.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/jerdog/Library/Logs/bluesky-notify.error.log</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
        <key>PYTHONUNBUFFERED</key>
        <string>1</string>
        <key>HOME</key>
        <string>/Users/jerdog</string>
    </dict>
    <key>WorkingDirectory</key>
    <string>/Users/jerdog</string>
</dict>
</plist>
